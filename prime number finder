#include <stdio.h>

int main()
{
    int n,i;
    printf("Enter number: \n");
    scanf("%d",&n);
    for ( i = 2; i <= n ; ++i)
    {
        if (n % i == 0 )
        {
            if ( n > i)
            {
                printf("it is not a prime number");
                break;
            }
            
            printf("It is a prime number");
        }
    } 
    return 0;
}
